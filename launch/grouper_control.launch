<launch>
  <node name="potision_controller_tutorials" pkg="dynamixel_workbench_controllers" type="dynamixel_workbench_position_control" required="true" output="screen">
    <rosparam>
      device_name_: "/dev/ttyUSB0"
      baud_rate_: 57600
      protocol_version_: 1.0
      motor_count_: 3
      motor_0_:
        motor_model_: "RX_10"
        motor_name_: "cam_pan"
        motor_id_: 2
      motor_1_:
        motor_model_: "RX_10"
        motor_name_: "cam_tilt"
        motor_id_: 3
      motor_2_:
        motor_model_: "MX_106"
        motor_name_: "radar_tilt"
        motor_id_: 1
   </rosparam>
  </node>
</launch>
