<launch>
  <node name="grouper_dynamixel_control" pkg="dynamixel_control" type="dynamixel_control" required="true" output="screen">
    <rosparam>
      device_name_: "/dev/ttyUSB0"
      baud_rate_: 57600
      protocol_version_: 1.0
      motor_count_: 2
      motor_0_:
        motor_name_: "cam_pan"
        motor_model_: "RX_10"
        motor_id_: 2
      motor_1_:
        motor_name_: "cam_tilt"
        motor_model_: "RX_10"
        motor_id_: 3
   </rosparam>
  </node>
</launch>
